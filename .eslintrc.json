{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "env": {
        "browser": true
    },
    "rules": {
      "react/jsx-filename-extension": 0
    },
    "settings": {
       "import/resolver": {
         "webpack": "webpack.config.js"
        }
    }
}
